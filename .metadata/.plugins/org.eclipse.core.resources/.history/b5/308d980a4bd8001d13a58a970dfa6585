package quiz01;


import java.util.Scanner;

public class AAA {
	
	public static void main(String[] args) {
		
		
	Scanner scan = new Scanner(System.in);
	
	System.out.print("> ");
	int t = scan.nextInt();
	int i = 0;
			
	System.out.print("출력수 ");
	
	while(t>=i) {
		
		if( t%3 ==0 || t%4 ==0) {
			System.out.print(t);
		}
	break;
	}
}
}