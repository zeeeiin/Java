package Quiz22;

public class Product {

	private String day;
	private String store;
	private String greade;
	private String detail;
	private String price;
	
	//생성자
	public Product() {}
		
	public Product(String day, String store, String greade, String detail, String price) {
		super();
		this.day = day;
		this.store = store;
		this.greade = greade;
		this.detail = detail;
		this.price = price;
	}

	
	//get/set
	public String getDya() {
		return day;
	}

	public void setDya(String dya) {
		this.day = day;
	}

	public String getStore() {
		return store;
	}

	public void setStore(String store) {
		this.store = store;
	}

	public String getGreade() {
		return greade;
	}

	public void setGreade(String greade) {
		this.greade = greade;
	}

	public String getDetail() {
		return detail;
	}

	public void setDetail(String detail) {
		this.detail = detail;
	}

	public String getPrice() {
		return price;
	}

	public void setPrice(String price) {
		this.price = price;
	}
	
	
	
	
	
}
